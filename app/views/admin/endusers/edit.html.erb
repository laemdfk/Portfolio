<h6>Admin::Endusers#edit</h6>

<%= form_with model: @enduser, url: admin_enduser_path(@enduser.id), method: :patch do |f| %>
<div class="container">
 <div class="row">

    <table class="table table-hover table-inverse">
      <tr>
       <td>会員ID</td>
       <td><%= @enduser.id %></td>
      </tr>

      <tr>
       <td>ニックネーム</td>
       <td><%= @enduser.nickname %></td>
      </tr>

      <tr>
        <td>メールアドレス</td>
        <td><%= @enduser.email %></td>
      </tr>


      <tr>
       <td><%= f.label :is_deleted,"会員ステータス" %></td>
       <td>
         <%= f.radio_button :is_deleted, false %>
         <%= f.label :is_deleted, "有効"  %>
         <%= f.radio_button :is_deleted, true %>
         <%= f.label :is_deleted, "無効" %>
       </td>
      </tr>
   </table>
     <%= f.submit '変更を保存', class:"btn btn-success" %><br />
      <% end %>
   </div>
 </div>